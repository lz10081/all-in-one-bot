<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="184" Margin="138,135,0,0" VerticalAlignment="Top" Width="510"/>

        <Label x:Name="label" Content="Please enter your activation key to gain access to the XXXXXXXXXX software." HorizontalAlignment="Left" VerticalAlignment="Top" Margin="170,178,0,0" Width="460"/>
        <Button x:Name="buttonkey"  Click="ButtonClicked" Content="AUTHORIZE" HorizontalAlignment="Left" VerticalAlignment="Top" Width="87" Margin="353,280,0,0"/>
        <TextBox x:Name="textBoxkey" HorizontalAlignment="Left" Height="23" Margin="266,229,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="276" />

    </Grid>
</Window>
